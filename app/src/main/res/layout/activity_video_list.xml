<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".videoList">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:orientation="horizontal"></LinearLayout>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:fontFamily="@font/tmoneyroundwindextrabold"
            android:gravity="center_horizontal"
            android:text="눈운동 영상"
            android:textColor="@drawable/mybutton_pink"
            android:textSize="50sp"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="426dp"
            android:layout_height="659dp"
            android:text="사용 거리 설정">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="141dp"
                android:layout_height="92dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.15"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.229"
                app:srcCompat="@drawable/tajo1" />

            <Button
                android:id="@+id/btn_back"
                android:layout_width="409dp"
                android:layout_height="67dp"
                android:background="@color/colorTitlePink"
                android:fontFamily="@font/tmoneyroundwindextrabold"
                android:text="뒤로 가기"
                android:textColor="@color/colorLightBeige"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.058"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView3"
                app:layout_constraintVertical_bias="0.423" />

            <Button
                android:id="@+id/tajo3_button"
                android:layout_width="103dp"
                android:layout_height="48dp"
                android:background="@drawable/mybutton"
                android:fontFamily="@font/tmoneyroundwindextrabold"
                android:onClick="onClick"
                android:text="활모양 운동 (4:05)"
                android:textColor="@color/colorSemiDarkGray"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.182"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.71" />

            <Button
                android:id="@+id/tajo4_button"
                android:layout_width="103dp"
                android:layout_height="48dp"
                android:background="@drawable/mybutton"
                android:fontFamily="@font/tmoneyroundwindextrabold"
                android:onClick="onClick"
                android:text="눈운동 모음 (9:33)"
                android:textColor="@color/colorSemiDarkGray"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.773"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.71" />

            <Button
                android:id="@+id/tajo1_button"
                android:layout_width="103dp"
                android:layout_height="48dp"
                android:background="@drawable/mybutton"
                android:fontFamily="@font/tmoneyroundwindextrabold"
                android:onClick="onClick"
                android:text="사선 운동 (1:45)"
                android:textColor="@color/colorSemiDarkGray"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.182"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.394" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="144dp"
                android:layout_height="268dp"
                android:layout_marginStart="36dp"
                android:layout_marginBottom="152dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/tajo3" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/tmoneyroundwindregular"
                android:gravity="center_horizontal"
                android:lineSpacingExtra="5dp"
                android:text="마음에 드는 영상을 골라 \n눈 운동을 해보세요!"
                android:textColor="@color/colorDarkGray"
                android:textSize="26sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.475"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.041" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="138dp"
                android:layout_height="179dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.805"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.587"
                app:srcCompat="@drawable/tajo4" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="140dp"
                android:layout_height="242dp"
                android:layout_marginTop="52dp"
                android:layout_marginEnd="44dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/tajo2" />

            <Button
                android:id="@+id/tajo2_button"
                android:layout_width="103dp"
                android:layout_height="48dp"
                android:background="@drawable/mybutton"
                android:fontFamily="@font/tmoneyroundwindextrabold"
                android:onClick="onClick"
                android:text="8자 운동 (3:00)"
                android:textColor="@color/colorSemiDarkGray"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.773"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.394" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>