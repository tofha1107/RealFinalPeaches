<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EyePage">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="413dp"
        android:layout_height="225dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/button12"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="100dp"
            android:layout_marginRight="100dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="20"
            android:textColor="@color/colorLightBeige"
            android:textSize="40sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button6"
            android:layout_width="57dp"
            android:layout_height="27dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="116dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="분마다"
            android:textColor="@color/colorLightBeige"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button11"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="10"
            android:textColor="@color/colorLightBeige"
            android:textSize="40sp"
            app:layout_constraintEnd_toStartOf="@+id/button12"
            app:layout_constraintHorizontal_bias="0.714"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button14"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="30"
            android:textColor="@color/colorLightBeige"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/button11"
            app:layout_constraintTop_toBottomOf="@+id/button11"
            app:layout_constraintVertical_bias="0.555" />

        <Button
            android:id="@+id/button7"
            android:layout_width="49dp"
            android:layout_height="19dp"
            android:layout_marginStart="20dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="분마다"
            android:textColor="@color/colorLightBeige"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/button11"
            app:layout_constraintTop_toBottomOf="@+id/button11"
            app:layout_constraintVertical_bias="0.742" />

        <Button
            android:id="@+id/button13"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="60"
            android:textColor="@color/colorLightBeige"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/button12"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/button14"
            app:layout_constraintTop_toBottomOf="@+id/button12" />

        <Button
            android:id="@+id/button16"
            android:layout_width="57dp"
            android:layout_height="19dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="분마다"
            android:textColor="@color/colorLightBeige"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/button12"
            app:layout_constraintHorizontal_bias="0.792"
            app:layout_constraintStart_toEndOf="@+id/button14"
            app:layout_constraintTop_toBottomOf="@+id/button12"
            app:layout_constraintVertical_bias="0.74" />

        <Button
            android:id="@+id/button4"
            android:layout_width="45dp"
            android:layout_height="27dp"
            android:layout_marginTop="76dp"
            android:background="@drawable/round"
            android:fontFamily="@font/tmoneyroundwindregular"
            android:text="분마다"
            android:textColor="@color/colorLightBeige"
            android:textSize="15sp"
            app:layout_constraintEnd_toStartOf="@+id/button12"
            app:layout_constraintHorizontal_bias="0.651"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="127dp"
        android:layout_height="108dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.06"
        app:srcCompat="@drawable/basic_peach" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/tmoneyroundwindregular"
        android:text="스마트폰으로 지친 내 아이의 눈,"
        android:textColor="@color/colorDarkGray"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="234dp"
        android:layout_height="65dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/tmoneyroundwindregular"
        android:gravity="center"
        android:text="간단히 따라할 수 있는 눈운동을 시키면 어떨까요?"
        android:textColor="@color/colorTitlePink"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <Button
        android:id="@+id/button_choose2"
        android:layout_width="409dp"
        android:layout_height="67dp"
        android:background="@color/colorTitlePink"
        android:fontFamily="@font/tmoneyroundwindextrabold"
        android:text="선택 완료!"
        android:textColor="@color/colorLightBeige"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="252dp"
        android:layout_height="88dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/tmoneyroundwindregular"
        android:gravity="center"
        android:text="아이가 스마트폰을 보는 동안 몇 분 간격으로 눈 운동을 수행할지 정해주세요!"
        android:textColor="@color/colorTitlePink"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

    <Button
        android:id="@+id/button8"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/bar"
        android:fontFamily="@font/tmoneyroundwindextrabold"
        android:text="STEP. 03"
        android:textColor="@color/colorLightBeige"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

</androidx.constraintlayout.widget.ConstraintLayout>